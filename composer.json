{
  "name": "lichi/huntflow-sdk",
  "type": "library",
  "description": "PHP SDK for HUNTFLOW",
  "keywords": ["Lichi","lichi", "huntflow php sdk", "huntflow", "huntflow api", "huntflow sdk"],
  "homepage": "https://github.com/IvanHunters/lichi-huntflow-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Okhotnikov",
      "email": "ohotnikovivan@yandex.ru"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "guzzlehttp/guzzle": "~6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Lichi\\Huntflow\\": "src/",
      "Lichi\\Huntflow\\Sdk\\": "src/sdk/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
